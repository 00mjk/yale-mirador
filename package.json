    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dexie": "^1.5.1",
    "golden-layout": "^1.5.9",
    "js-cookie": "^2.2.0",
    "semantic-ui": "^2.2.13"
  }
