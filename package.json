{
  "name": "yale-mirador",
  "version": "0.8.2",
  "description": "Yale Extension of Mirador",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "karma start",
    "build": "npm run package:mirador && npm run build:yale-mirador && ./bin/package-lib.sh",
    "package:mirador": "rm -rf dist/mirador && mkdir dist/mirador && cp -r node_modules/mirador-y/dist/mirador/* dist/mirador/",
    "prebuild:yale-mirador": "rm -rf dist/yale-mirador && mkdir -p dist/yale-mirador",
    "build:yale-mirador": "GIT_DESC=$(git describe --long --always) webpack --progress --colors --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yale-web-technologies/yale-mirador.git"
  },
  "author": "Seong-June Kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yale-web-technologies/yale-mirador/issues"
  },
  "homepage": "https://github.com/yale-web-technologies/yale-mirador#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.7",
    "exports-loader": "^0.6.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^3.9.1",
    "imports-loader": "^0.7.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "joosugi": "^0.3.2",
    "joosugi-semantic-ui": "^0.1.2",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mirador-y": "^2.6.0-1",
    "mocha": "^4.0.1",
    "npm-version": "^1.1.0",
    "phantomjs": "^2.1.7",
    "pug": "^2.0.0-rc.4",
